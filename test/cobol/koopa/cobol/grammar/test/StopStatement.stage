+[ STOP RUN WITH NORMAL        42        ]
+[ STOP RUN WITH NORMAL                  ]
+[ STOP RUN      NORMAL        my-status ]
+[ STOP RUN      NORMAL        "success" ]
+[ STOP RUN      NORMAL        42        ]
+[ STOP RUN      NORMAL                  ]
