+[ STOP RUN WITH NORMAL STATUS           ]
+[ STOP RUN      NORMAL STATUS my-status ]
+[ STOP RUN      NORMAL STATUS "success" ]
+[ STOP RUN      NORMAL STATUS 42        ]
+[ STOP RUN      NORMAL STATUS           ]
