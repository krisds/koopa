package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target readyTraceStatement;

+[ READY TRACE ]

target resetTraceStatement;

+[ RESET TRACE ]

target useStatement;

+[ USE DEBUGGING "foo" . ]
