package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target entryStatement;

-[ ENTRY ]

+[ ENTRY "MAIN-ENTRANCE" ]

+[ ENTRY "MAIN-ENTRANCE" M-E ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A FIELD-B ]

+[ ENTRY "MAIN-ENTRANCE" M-E
         USING FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE"
         # FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         # USING FIELD-B ]

+[ ENTRY "MAIN-ENTRANCE" USING
         REFERENCE FIELD-A ]
         
+[ ENTRY "MAIN-ENTRANCE" USING
         BY REFERENCE FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE" USING
         BY REFERENCE FIELD-A DELIMITED ]

+[ ENTRY "MAIN-ENTRANCE" USING
         BY REFERENCE FIELD-A DELIMITED BY SIZE ]

+[ ENTRY "MAIN-ENTRANCE" USING
         BY REFERENCE ANY ]

+[ ENTRY "MAIN-ENTRANCE" USING
         VALUE FIELD-A ]
         
+[ ENTRY "MAIN-ENTRANCE" USING
         BY VALUE FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE" USING
         BY VALUE ANY ]

+[ ENTRY "MAIN-ENTRANCE" USING
         BY REFERENCE FIELD-A FIELD-B
         BY VALUE FIELD-C FIELD-D ]
         
+[ ENTRY "MAIN-ENTRANCE" 
         USING FIELD-A
         REPEATED ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         REPEATED 1 TO 100 ]
         
+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         GIVING FIELD-B ]

+[ ENTRY "MAIN-ENTRANCE"
         GIVING FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         RETURNING FIELD-B ]

+[ ENTRY "MAIN-ENTRANCE"
         RETURNING FIELD-A ]

+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         REPEATED 1 TO 100 ]
         
+[ ENTRY "MAIN-ENTRANCE"
         USING FIELD-A
         REPEATED 1 TO 100
         GIVING FIELD-B ]

