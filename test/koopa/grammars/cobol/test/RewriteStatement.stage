package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target rewriteStatement;

+[ REWRITE MY-RECORD # . ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER # . ]

+[ REWRITE MY-RECORD
   END-REWRITE ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   END-REWRITE ]

+[ REWRITE MY-RECORD 
   INVALID KEY
      DISPLAY "Oops." ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   INVALID KEY
      DISPLAY "Oops." ]

+[ REWRITE MY-RECORD 
   INVALID
      DISPLAY "Oops." ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   INVALID
      DISPLAY "Oops." ]

+[ REWRITE MY-RECORD 
   NOT INVALID KEY
      DISPLAY "A-OK." ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   NOT INVALID KEY
      DISPLAY "A-OK." ]

+[ REWRITE MY-RECORD 
   NOT INVALID
      DISPLAY "A-OK." ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   NOT INVALID
      DISPLAY "A-OK." ]

+[ REWRITE MY-RECORD 
   INVALID KEY
      DISPLAY "Oops." 
   END-REWRITE ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   INVALID KEY
      DISPLAY "Oops." 
   END-REWRITE ]

+[ REWRITE MY-RECORD 
   INVALID
      DISPLAY "Oops." 
   END-REWRITE ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   INVALID
      DISPLAY "Oops." 
   END-REWRITE ]

+[ REWRITE MY-RECORD 
   NOT INVALID KEY
      DISPLAY "A-OK." 
   END-REWRITE ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   NOT INVALID KEY
      DISPLAY "A-OK." 
   END-REWRITE ]

+[ REWRITE MY-RECORD 
   NOT INVALID
      DISPLAY "A-OK." 
   END-REWRITE ]

+[ REWRITE MY-RECORD FROM MY-IDENTIFIER
   NOT INVALID
      DISPLAY "A-OK." 
   END-REWRITE ]
